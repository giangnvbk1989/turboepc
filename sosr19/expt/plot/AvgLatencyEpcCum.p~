#!/bin/bash
set autoscale #scales axes automatically
unset log #remove any log-scaling
unset label #remove any prev labels
set terminal postscript eps enhanced color font 'Arial,17'
set output 'AvgLatencyEPCCum.eps'
#set xtic 3
set ytic auto
#set title "LTE-EPC: Application End-to-end Delay(ms)"
set xlabel "Traffic Mix Ratios- Non-offloadable:Offloadable"
set ylabel "Average Response Time (ms)"

set style fill pattern 4
set style data histogram
set style histogram gap 2

set size 1,0.7
#set key at 2,44
#set xrange [0:]
set yrange[0:25]
#set xtics border in scale 0,0 nomirror rotate by 45  offset character -1, -4, 0
plot newhistogram fs pattern 1, 'cent-wo-data.dat' using 3:xtic(1) title 'Traditional EPC' lc rgb 'red' lw 2  ,\
    'offload-wo-data.dat' using 3 title 'TurboEPC' lc rgb 'black' lw 2  #, \
    # 'cuttlefish.dat' using 3 title 'Cuttlefish' lc rgb 'medium-blue' lw 2

unset style data
unset style histogram
